*{font-family: "微软雅黑";font-size: 16px;}

.carousel-inner img{
	margin: 0 auto;
}
.bc-social{
	padding: 15px 0;
	text-align: center;
	background-color: #F5F5F5;
	border-top: 1px solid #fff;
	border-bottom: 1px solid #ddd;
}
.bc-social-buttons{
	margin-left: 0;
	margin-bottom: 0;
	padding-left: 0;
	list-style: none;
}
.social-forum{
	display: inline-block;
	line-height: 1;
	color: #555;
}
.bc-social-buttons>li+li:before{
	padding: 0 10px;
	color: #ccc;
	content: "|";
}
.btn-group-vertical>.btn-group:after, .btn-toolbar:after, .clearfix:after, .container-fluid:after, .container:after, .dl-horizontal dd:after, .form-horizontal .form-group:after, .modal-footer:after, .nav:after, .navbar-collapse:after, .navbar-header:after, .navbar:after, .pager:after, .panel-body:after, .row:after{
	clear: both;
}
.bc-social-buttons li{
	display: inline-block;
	line-height: 1;
	color: #555;
}
.projects{
	margin: 0 auto;
}
.projects-header{
	width: 60%;
	text-align: center;
	margin: 60px 0  10px;
	font-weight: 200;
	margin-bottom: 40px;
	display: block;
	margin-left: auto;
	margin-right: auto;
}
.footer-top{
	border-bottom: 1px solid #9D9D9D;
	margin-left: auto;
	margin-right: auto;
}
p{
	margin: 0 0 10px;
}
a{
	color: #337AB7;
	text-decoration: none;
}
.id-a{
	display: block;
	width: 780px;
	height: 440px;
	position: absolute;
	margin-left: 170px;
	
}
.carousel-inner>.item>img{
	height:440px;
}
.banner-wrap{position: relative;}
.banner-wrap:hover .float{
	width:248.5px ;
}
.float{
	position: absolute;
	opacity: 0.8;	
	background: #201512;
	top: 0;left: 0px;
	height: 344.8px;
	transition:0.8s;overflow: hidden;
	width: 0;
}
.serial{
	display: block;
	width: 47px;
	height: 47px;
	padding-top: 10px;
	border-radius: 24px;
	background: #FFD564;
	border: solid 3px #fff;
	font: 18px 'roboto' sans-serif;
	color: #3071A9;
	font-weight: 900;
	text-align: center;
	margin-left: 20px;
	margin-top: 20px;
	opacity: 0.8;	
}
.float-title{
	display: block;
	margin-left: 85px;
	width: 120px;
	color: #FE5660;
}
.float-min{
	color: #fff;
	font-size: 16px;
	clear: both;
	margin-left:85px;
	padding-top:15px;
	width: 120px;	
}
.float-vote{
	color: #fff;
	font-size: 15px;
	clear: both;
	margin-left: 85px;
	width: 120px;
}
.float-a{
	font-size: 15px;
	color: #4C4145;
	background: #FFD564;
	border-radius: 5px;
	margin-left:83px;
	padding: 5px;
	display: block;
	margin-top: 30px;
	width: 95px;
}
.float-option{
	color: #fff;
	font-size: 15px;
	clear: both;
	margin-left: 85px;
	width: 125px;
}
.goupiao{
	position: absolute;
	color: #fff;
	top: 135px;
	left: 15px;
	display: block;
	padding: 5px;
	background:#fe505a ;
	border-radius: 5px;
}
.goupiao:hover{
	color: #fff;
}
.touxiang{
	width: 40px;
	height:40px;
	position: relative;
	right: 5px;
	top:-1px;
	border-radius: 20px;
}
