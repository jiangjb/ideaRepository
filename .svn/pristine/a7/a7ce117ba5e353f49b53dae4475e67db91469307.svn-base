.wrapper {
  overflow: hidden;
  position: relative;
  min-height: 100%;
}
.wrapper-images {
  background-image: url(../images/bg-image.jpg);
  -webkit-background-size: cover;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  background-color: #B1B2B4;
}
.container {
  margin-right: auto;
  margin-left: auto;
  padding-left: 15px;
  padding-right: 15px;
  position: relative;
}
@media (min-width: 768px) {
  .container {
    width: 750px;
  }
}
@media (min-width: 992px) {
  .container {
    width: 970px;
  }
}
@media (min-width: 1200px) {
  .container {
    width: 1170px;
  }
}
.container {
  padding: 0;
}
@-ms-viewport {
  width: device-width;
}
.header-wrapper {
  background: #2e292e;
  position: relative;
  z-index: 120;
}
.header-wrapper--home {
  background: rgba(46, 41, 46, 0.8);
  position: absolute;
  height: 55px;
  width: 100%;
  z-index: 101;
}
.footer-wrapper {
  background:#575357;
  background-repeat: repeat;
  overflow: hidden;
  margin-top: 60px;
}
.banner-wrap {
  margin: 10px 0;
}
.footer-info {
  padding-top: 64px;
}
.logo {
  position: absolute;
  top: 16px;
  left: 15px;
  z-index: 2;
}
.logo img {
  width: 111px;
  height: 17px;
}
#navigation-box {
  background-color: transparent;
  margin: 0;
  min-height: 55px;
}
ul#navigation {
  margin-right: 100px;
}
ul#navigation a {
  font: 17px '微软雅黑', sans-serif;
  color: #b4b1b2;
  -webkit-transition: 0.3s;
  transition: 0.3s;
}
ul#navigation a:hover {
  color: #ffffff;
}
.copy {
  font-family: 'aleoitalic', sans-serif;
  font-size: 15px;
  padding-bottom: 45px;
  color: #FFFFFF;
  margin: 0 auto;
  width:400px;
  margin-top: 20px\n;
}
.copy-bottom {
  position: absolute;
  bottom: 23px;
  width: 100%;
}
.copy-bottom .copy {
  padding-bottom: 0;
  margin-bottom: 0;
  text-align: center;
}
.regist-container {
	margin-top: 40px;
	padding-top: 93px;
}
			
 .input-file-img{
	 vertical-align: middle;
	 margin-left: 25px; 
	 width: 60px; 
	 height: 60px;
	 line-height:60px;
	 border-radius:30px;
	 border: 2px solid #FFD564;
}
 .input-file{
 	position: relative;
 	cursor: pointer;
    color: #888;
    background: #fafafa;
    border: 1px solid #ddd;
    border-radius: 4px;
    overflow: hidden;
    display: inline-block;
    *display: inline;
    *zoom: 1;
    margin-left: 38px;
    width: 230px;
    text-align: center;
    margin-top: 7px;
    
}
 .input-file input{
 	 position: absolute;
 	 font-size: 100px;
     right: 0;
     top: 0;
     opacity: 0;
    filter: alpha(opacity=0);
    cursor: pointer;
}
 .input-file:hover{
 	color: #444;
    background: #eee;
    border-color: #ccc;
    text-decoration: none
 }